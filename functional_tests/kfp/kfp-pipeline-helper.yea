id: kfp.pipeline.helper
tag:
    shard: kfp
plugin:
  - wandb
depend:
  requirements:
    - kfp==1.8.11
assert:
    - :wandb:runs_len: 3
    - :wandb:runs[0][summary]: {}
    - :wandb:runs[1][summary]: {}
    - :wandb:runs[2][summary]: {}
    - :op:contains:
      - :wandb:runs[0][telemetry][3]  # feature
      - 36  # kfp_wandb_log
    - :op:contains:
      - :wandb:runs[1][telemetry][3]  # feature
      - 36  # kfp_wandb_log
    - :op:contains:
      - :wandb:runs[2][telemetry][3]  # feature
      - 36  # kfp_wandb_log
